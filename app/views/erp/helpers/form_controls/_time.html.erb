<div class="form-group">
    <label class="control-label"><%= options[:label] if options[:label].present? %></label>
    <div class="input-icon">
        <i class="fa fa-clock-o"></i>
        <input class="form-control timepicker timepicker-no-seconds"
            type="text"
            name="<%= options[:name] if options[:name].present? %>"
            value="<%= options[:value] if options[:value].present? %>"
        />
    </div>
</div>